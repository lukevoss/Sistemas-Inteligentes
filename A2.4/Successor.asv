function [list] = Successor(actual)
%% Function that creates all the Successors from a actual state
N = length(actual);
listCosts = zeros(1,N);
for i = 1:N
    for j = i+1:N
        actual([i j]) = actual ([j i]);%swap
        list = [list ; actual];
        listCosts( = [listCosts 
    end
end
end

